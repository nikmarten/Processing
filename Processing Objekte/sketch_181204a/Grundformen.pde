abstract class Grundformen {

  
  int segmentsCircle;
  float height;
  float radius;
  
  public void setRadius(float r){
    radius = r;  
  }
  
  public float getRadius(){
    return radius;
  }
  
  public void setBodyHeight(float h){
    height = h;
  }
  
  public float getBodyHeight(){
   return height;
 }
 
 public void setSegmentsTop(int s){
   segmentsCircle = s;
 }
 
 public int getSegmentsTop(){
   return segmentsCircle;
 }
}
